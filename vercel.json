{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"

    }
  ],
  "routes": [
  {
      "src": "/api/v1/payment",
      "dest": "/api/index.js",
      "methods": ["POST"],
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ]
}